        "target": "ESNext",
        "lib": ["DOM", "DOM.Iterable", "ESNext"],
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "skipLibCheck": true,
        "noEmit": true,
        "strict": false,
        "allowJs": true,
        "jsx": "react-jsx"
    },
    "include": ["src"]
